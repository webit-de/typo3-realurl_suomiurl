{
  "name": "webit-de/realurl-suomiurl",
  "description": "This TYPO3 CMS extension adds a hook for special conversion of suomi characters in URL segments (Finland)",
  "homepage": "https://github.com/webit-de/typo3-realurl_suomiurl/",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
    "php": ">=7.0.0",
    "typo3/cms-core": "^6.2 || ^7.6 || ^8.7",
    "dmitryd/typo3-realurl": "^2.1"
  },
  "replace": {
    "realurl_suomiurl": "self.version",
    "typo3-ter/realurl-suomiurl": "self.version"
  }
}
